<h1>Sign In</h1>

<form action="<%= session_url %>" method="POST">

  <label for="username">Username:
    <input type="text" name="users[user_name]">
  </label>
  <label for="password">Password:
    <input type="password" name="users[password]">
  </label>
  <input type="submit" value="Submit">
</form>